{
    "name": "@anglr/md-help",
    "version": "0.0.0-placeholder",
    "description": "Angular module containing basic implementation of markdown help interpreter",
    "main": "dist/index.js",
    "sideEffects": false,
    "scripts": {
        "build": "npm run build:node && npm run build:web",
        "build:node": "copyfiles -u 1 src/index.js dist",
        "build:web": "ngc -p web/tsconfig.es5.json && ngc -p web/tsconfig.es2015.json",
        "clean": "rimraf dist tests **/tsdoc-metadata.json web/**/*.d.ts web/**/*.d.ts.map */*.metadata.json ./*.metadata.json ./*.d.ts ./*d.ts.map */dist",
        "clean:duplicates": "rimraf */dist/*/src",
        "clean:deps": "rimraf package-lock.json node_modules",
        "clean:all": "npm run clean && npm run clean:deps",
        "ngc": "ngc",
        "version:placeholder": "replace-in-file /\\\"version\\\":\\s\\\".*?\\\"/g \"\\\"version\\\": \\\"0.0.0-placeholder\\\"\" package.json --isRegex",
        "version:git": "ngv -e \"replace-in-file 0.0.0-placeholder $GIT_VERSION package.json && echo $GIT_VERSION > version.bak\"",
        "version:build": "ngv -e \"echo $GIT_VERSION > version.bak\"",
        "prepublishOnly": "npm run bundle && npm run version:placeholder && npm run version:git",
        "postpublish": "npm run doc && npm run version:placeholder && npm run clean",
        "bundle": "npm run clean && npm run build && npm run clean:duplicates",
        "deploy": "npm publish --loglevel error",
        "deploy:next": "npm publish --tag next --loglevel error",
        "pretest": "webpack",
        "test": "karma start karma.conf.js",
        "doc": "path-exists docs && npm run doc:generate || echo \"No docs generated!\"",
        "doc:generate": "npm run doc:generate:web && npm run doc:save",
        "doc:save": "cd docs && git add . && git commit -m \"INT: updated API docs ng-md-help\" && git push && cd ..",
        "doc:generate:web": "rimraf docs/content/api/ng-md-help && npm run doc:json:web && npm run doc:md:web",
        "doc:json:web": "api-extractor run --local --verbose -c web/api-extractor.json",
        "doc:md:web": "api-documenter markdown -i web/temp -o docs/content/api/ng-md-help && rimraf web/temp"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ressurectit/ng-md-help.git"
    },
    "keywords": [
        "angular",
        "ng",
        "markdown",
        "help"
    ],
    "author": "kukjevov",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ressurectit/ng-md-help/issues"
    },
    "homepage": "https://github.com/ressurectit/ng-md-help",
    "peerDependencies": {
        "@angular/core": ">=9.0.0",
        "@angular/common": ">=9.0.0",
        "@angular/router": ">=9.0.0",
        "rxjs": ">=6.5.0",
        "@jscrpt/common": ">=1.1.1",
        "@anglr/notifications": ">=7.0.0",
        "marked": ">=0.3.19",
        "highlight.js": ">=9.12.0",
        "gulp": ">=3.9.1",
        "gulp-compile-handlebars": ">=0.6.1",
        "read-file": ">=0.2.0",
        "gulp-file": ">=0.4.0",
        "through2": ">=2.0.3"
    },
    "devDependencies": {
        "@angular/core": "^9.0.0",
        "@angular/common": "^9.0.0",
        "@angular/router": "^9.0.0",
        "@angular/animations": "^9.0.0",
        "rxjs": "^6.5.0",
        "zone.js": "^0.8.26",
        "@types/numeral": "~0.0.22",
        "@types/marked": "0.3.0",
        "@types/highlight.js": "9.12.2",
        "@jscrpt/common": "^1.1.1",
        "@anglr/notifications": "^7.0.0",
        "@anglr/animations": "^7.0.0",
        "numeral": "2.0.6",
        "@types/node": "10.17.0",
        "@types/jasmine": "~2.8.7",
        "@angular/compiler-cli": "^9.0.0",
        "@angular/compiler": "^9.0.0",
        "@angular/language-service": "^9.0.0",
        "@angular/platform-browser-dynamic": "^9.0.0",
        "@angular/platform-browser": "^9.0.0",
        "@microsoft/api-extractor": "7.7.8",
        "@microsoft/api-documenter": "7.7.12",
        "tsickle": "0.38.0",
        "karma-cli": "1.0.1",
        "karma": "3.1.1",
        "karma-jasmine": "1.1.2",
        "karma-phantomjs-launcher": "1.0.4",
        "karma-chrome-launcher": "2.2.0",
        "karma-webpack": "3.0.5",
        "karma-jasmine-html-reporter": "1.4.0",
        "jasmine": "3.3.0",
        "awesome-typescript-loader": "5.2.1",
        "angular2-template-loader": "0.6.2",
        "raw-loader": "1.0.0",
        "sass-loader": "7.1.0",
        "css-loader": "1.0.1",
        "file-loader": "3.0.1",
        "style-loader": "0.23.1",
        "rimraf": "2.6.3",
        "copyfiles": "2.1.0",
        "replace-in-file": "4.1.0",
        "path-exists-cli": "1.0.0",
        "typescript": "3.7.5",
        "webpack": "4.32.2",
        "npm-git-version": "1.1.1"
    }
}
